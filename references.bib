pmid = {22542243},
publisher = {Elsevier B.V.},
title = {{Medial temporal lobe structure and cognition in individuals with schizophrenia and in their non-psychotic siblings.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22542243},
volume = {138},
