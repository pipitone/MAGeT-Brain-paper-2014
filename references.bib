journal = {IEEE transactions on medical imaging},
keywords = {Algorithms,Brain,Brain: anatomy \& histology,Decision Making, Computer-Assisted,Humans,Image Enhancement,Image Enhancement: methods,Image Interpretation, Computer-Assisted,Image Interpretation, Computer-Assisted: methods,Infant, Newborn,Magnetic Resonance Imaging,Magnetic Resonance Imaging: methods,Markov Chains,Models, Statistical,Observer Variation,Phantoms, Imaging,Reproducibility of Results,Sensitivity and Specificity,maget},
mendeley-tags = {maget},
