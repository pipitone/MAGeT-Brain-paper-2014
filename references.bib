issn = {0278-0062},
journal = {IEEE transactions on medical imaging},
keywords = {Affective Disorders, Psychotic,Affective Disorders, Psychotic: diagnosis,Algorithms,Artificial Intelligence,Brain,Brain: pathology,Computer Simulation,Humans,Image Enhancement,Image Enhancement: methods,Image Interpretation, Computer-Assisted,Image Interpretation, Computer-Assisted: methods,Magnetic Resonance Imaging,Magnetic Resonance Imaging: methods,Models, Neurological,Pattern Recognition, Automated,Pattern Recognition, Automated: methods,Reproducibility of Results,Schizophrenia,Schizophrenia: diagnosis,Sensitivity and Specificity},
