journal = {Human brain mapping},
keywords = {Brain,Brain Diseases,Brain Diseases: genetics,Brain Diseases: pathology,Brain Diseases: physiopathology,Brain: anatomy \& histology,Brain: physiology,Diagnostic Imaging,Diagnostic Imaging: methods,Genetic Linkage,Humans,Phenotype},
