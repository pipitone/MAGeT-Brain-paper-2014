journal = {NeuroImage},
keywords = {Adult,Aged,Aged, 80 and over,Algorithms,Brain Mapping,Brain Mapping: methods,Female,Hippocampus,Hippocampus: pathology,Humans,Image Interpretation, Computer-Assisted,Image Interpretation, Computer-Assisted: methods,Magnetic Resonance Imaging,Male,Middle Aged},
