file = {:home/jp/Documents/Mendeley Desktop/Knickmeyer et al. - 2013 - Common Variants in Psychiatric Risk Genes Predict Brain Structure at Birth.pdf:pdf},
issn = {1460-2199},
journal = {Cerebral cortex (New York, N.Y. : 1991)},
keywords = {catechol-o-methyltransferase,cortex,disrupted-in-,neonate,neuroimaging,schizophrenia-1},
month = jan,
pages = {1--17},
pmid = {23283688},
title = {{Common Variants in Psychiatric Risk Genes Predict Brain Structure at Birth.}},
