journal = {NeuroImage},
keywords = {Adult,Brain,Brain Mapping,Brain Mapping: methods,Brain: physiopathology,Female,Humans,Image Interpretation, Computer-Assisted,Image Interpretation, Computer-Assisted: methods,Magnetic Resonance Imaging,Magnetic Resonance Imaging: methods,Male,Schizophrenia,Schizophrenia: physiopathology},
month = feb,
