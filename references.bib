keywords = {Adolescent,Adult,Atrophy,Biological Markers,Brain Mapping,Cephalometry,Cerebrospinal Fluid,Cerebrospinal Fluid: physiology,Disease Susceptibility,Dominance, Cerebral,Dominance, Cerebral: physiology,Female,Hippocampus,Hippocampus: pathology,Hippocampus: physiopathology,Humans,Image Processing, Computer-Assisted,Imaging, Three-Dimensional,Magnetic Resonance Imaging,Male,Schizophrenia,Schizophrenia: diagnosis,Schizophrenia: pathology,Schizophrenia: physiopathology,Schizophrenic Psychology,Statistics as Topic},
