journal = {Schizophrenia research},
keywords = {Databases, Factual,Databases, Factual: statistics \& numerical data,Frontal Lobe,Frontal Lobe: physiopathology,Humans,MEDLINE,MEDLINE: statistics \& numerical data,Meta-Analysis as Topic,Schizophrenia,Schizophrenia: pathology,Schizophrenia: physiopathology,anosognosia},
mendeley-tags = {anosognosia},
month = sep,
number = {1-3},
pages = {54--70},
pmid = {16837168},
title = {{Insight and frontal cortical function in schizophrenia: a review.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/16837168},
volume = {86},
year = {2006}
}
