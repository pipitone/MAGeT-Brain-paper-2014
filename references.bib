file = {:home/jp/Documents/Mendeley Desktop//L\"{o}tj\"{o}nen et al. - 2010 - Fast and robust multi-atlas segmentation of brain magnetic resonance images.pdf:pdf},
issn = {1095-9572},
journal = {NeuroImage},
keywords = {80 and over,Aged,Algorithms,Atlases as Topic,Brain,Brain: anatomy \& histology,Computer-Assisted,Computer-Assisted: methods,Female,Humans,Image Processing,Magnetic Resonance Imaging,Male,Middle Aged},
month = mar,
