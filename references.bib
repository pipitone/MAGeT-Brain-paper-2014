doi = {10.1016/j.neuroimage.2010.04.193},
file = {:home/jp/Documents/Mendeley Desktop//Collins, Pruessner - 2010 - Towards accurate, automatic segmentation of the hippocampus and amygdala from MRI by augmenting ANIMAL with a template library and label fusion.pdf:pdf},
