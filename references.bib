}
@article{Silberberg2008,
abstract = {Inhibitory pathways in the neocortex display a variety of temporal and spatial patterns, maintaining a dynamic balance with excitatory synaptic activity. Recent studies have revealed prevalent polysynaptic subcircuits within the neocortical microcircuitry. These subcircuits involve excitatory and inhibitory connections that are activated by neurons both in supragranular and infragranular cortical layers and mediated by different mechanisms. Interestingly, in these subcircuits inhibition is induced by discharge of pyramidal cells, and excitation is caused by specific types of GABAergic interneurons. The different polysynaptic subcircuits are discussed with respect to their spatial and temporal properties and their possible functional role in cortical processing.},
