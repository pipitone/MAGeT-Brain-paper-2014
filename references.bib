keywords = {Adult,Antipsychotic Agents,Antipsychotic Agents: therapeutic use,Awareness,Clozapine,Clozapine: therapeutic use,Cognition,Cognition: physiology,Delusions,Delusions: psychology,Drug Resistance,Electroconvulsive Therapy,Electroencephalography,Executive Function,Executive Function: physiology,Humans,Male,Neuropsychological Tests,Schizophrenia,Schizophrenia: therapy,Schizophrenic Psychology,Torticollis,Torticollis: etiology,anosognosia},
mendeley-tags = {anosognosia},
month = sep,
number = {3},
pages = {247--50},
pmid = {20966768},
title = {{Transient insight induction with electroconvulsive therapy in a patient with refractory schizophrenia: a case report and systematic literature review.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/20966768},
volume = {27},
year = {2011}
