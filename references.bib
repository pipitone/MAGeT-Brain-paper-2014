keywords = {linear mixed effects models,longitudinal studies,statistical analysis},
month = oct,
pages = {249--260},
pmid = {23123680},
publisher = {Elsevier Inc.},
title = {{Statistical analysis of longitudinal neuroimage data with Linear Mixed Effects models.}},
