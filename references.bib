journal = {Cerebral cortex (New York, N.Y. : 1991)},
keywords = {Adolescent,Biological Evolution,Child,Cognition,Female,Humans,Magnetic Resonance Imaging,Male,Organ Size,Prefrontal Cortex,Prefrontal Cortex: anatomy \& histology,Prefrontal Cortex: growth \& development,Species Specificity,Young Adult},
month = oct,
number = {10},
pages = {2352--7},
pmid = {18267953},
title = {{Brain size and folding of the human cerebral cortex.}},
